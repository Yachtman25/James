<!--Counting -->

<category>
    <pattern>COUNT TO<set>number</set></pattern>
    <template><think><set var="count">0</set>
    </think>
    <condition var="count">
    <li><value><star/></value></li>
    <li>
    <set var="count">
        <map><name>successor</name><get var="count"/></map>
    </set>
    <loop/></li>
    </condition>
    </template>
</category>

